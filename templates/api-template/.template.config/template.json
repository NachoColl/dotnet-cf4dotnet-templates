{
    "$schema": "http://json.schemastore.org/template",
    "author": "Nacho Coll",
    "tags": {
        "language": "C#"   
    },
    "classifications": [ "AWS", "Cloudformation", "C#" ],
    "identity": "NachoColl.Cloudformation4dotNET.Templates.cf4dotnet",
    "name": "AWS API Gateway and Lambda demo project to use with Cloudformation4dotNET (cf4dotnet).",
    "shortName": "cf4dotnet",
    "preferNameDirectory" : false,
    "sourceName": "MyAPI",
    "symbols":{
        "assemblyName": {
            "type": "parameter",
            "defaultValue": "MyProjectAssemblyName",
            "replaces":"MyProjectAssemblyName"
        },
        "tagCode": {
            "type": "parameter",
            "defaultValue": "MyProjectTagCode",
            "replaces":"MyProjectTagCode"
        }
    }
}